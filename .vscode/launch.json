{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node.js - Debug Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "Start server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start:server"
      ]
    },
    {
      "name": "Debug server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start:dev"
      ]
    }
  ]
}
